    <Grid>

        <SupplementProcessor:Vector2DField x:Name="Vector2DPosition" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Caption="Position" Height="29" Width="258" X="10" Y="10" XLabelContent="X" YLabelContent="Y"/>
        <Button x:Name="ButtonAdd" Content="OK" HorizontalAlignment="Left" Margin="138,81,0,0" VerticalAlignment="Top" Width="75"  Click="ButtonAdd_Click" />
        <Button x:Name="ButtonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="218,81,0,0" VerticalAlignment="Top" Width="75"  Click="ButtonCancel_Click" />
        <Label Content="Stroke Thickness" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBoxStroke" HorizontalAlignment="Left" Height="23" Margin="110,44,0,0" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" Width="39"/>

    </Grid>
</Window>
